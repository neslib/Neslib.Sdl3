[Project]
HeaderFileDirectory=SDL3
IncludeSubdirs=0
TargetPasFile=..\Neslib.Sdl3.Api.pas
UseUnits=
LibraryConstant=LIB_SDL3
DebugDefine=
ExcludedHeaders=
CTypesToDelphiMap=

[Platform.Win32]
Enabled=1
LibraryName=Sdl3_win32.dll
DebugLibraryName=
Prefix=

[Platform.Win64]
Enabled=1
LibraryName=Sdl3_win64.dll
DebugLibraryName=
Prefix=

[Platform.MacARM]
Enabled=1
LibraryName=libSdl3_macos_arm.a
DebugLibraryName=
Prefix=

[Platform.MacIntel]
Enabled=1
LibraryName=libSdl3_macos_intel.a
DebugLibraryName=
Prefix=

[Platform.Linux64]
Enabled=0
LibraryName=libSdl3.so
DebugLibraryName=
Prefix=

[Platform.iOS]
Enabled=1
LibraryName=libSdl3_ios.a
DebugLibraryName=
Prefix=

[Platform.Android32]
Enabled=1
LibraryName=libSdl3_android32.a
DebugLibraryName=
Prefix=

[Platform.Android64]
Enabled=1
LibraryName=libSdl3_android64.a
DebugLibraryName=
Prefix=

[ParseOptions]
IgnoreParseErrors=0
CmdLineArgs=-I.\,-DSDL_HIDAPI_DISABLED=1

[ConvertOptions]
CallConv=Cdecl
CharConvert=UTF8Char
UnsignedCharConvert=Byte
CommentConvert=KeepAsIs
ReservedWordHandling=PrefixAmpersand
TreatDirectivesAsReservedWords=1
PrefixSymbolsWithUnderscore=0
DelayedLoading=0
EnumHandling=ConvertToConst
UnconvertibleHandling=WriteToDo

[Ignore]
Count=0

[PostProcess]
Script=00000000

